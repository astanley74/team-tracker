<%= form_for @team do |f| %>

    <%= f.label :city %>
    <%= f.text_field :city %><br><br>

    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :coach %>
    <%= f.text_field :coach %><br><br>

    <%= f.hidden_field :user_id, value: @user.id %>

    <%= f.collection_select :league_id, League.all, :id, :name %><br><br>
    <%= f.submit %>
<% end %>